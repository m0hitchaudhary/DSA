#include <bits/stdc++.h>
using namespace std;
struct area {
    int sd;
    int len, wid;
};

void find_area(int side, int le, int wd) {

    area a1;
    
    a1.sd = side;
    a1.len = le;
    a1.wid = wd;
    
    cout<<a1.sd*a1.sd<<" ";
  
    cout<<a1.len*a1.wid;
}

int main(){
  int t;
  cin>>t;

  while(t--){
    int side, length, wide;
    cin>>side>>length>>wide;

    find_area(side, length, wide);
    cout<<endl;
  }
  return 0;
}
